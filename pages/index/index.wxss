.testWrap{
    font-size: 40rpx;
    text-align: center;
    margin: 30rpx;
}
.nowrap1 {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

page{
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.pageWrap{
    width: 750rpx;
    height: 100%;
    border-radius: 8px;
    position: relative;
}

.bg {
    height: 100%;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: -1;
}

.bg image {
    width: 100%;
    height: 100%;
}

.search{
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 100rpx;
    width: 261rpx;
	height: 75rpx;
	background-image: linear-gradient(90deg, 
		rgba(12, 89, 169, 0.21) 0%, 
		#ffffff 100%), 
	linear-gradient(
		#fcffff, 
		#fcffff);
	background-blend-mode: normal, 
		normal;
	border-radius: 38rpx;
    color: #07277a;
    font-size: 32rpx;
    line-height: 75rpx;
    text-align: center;
}

.param{
    width: 90%;
    height: 50%;
    background-color: #fff;
    border-radius: 20rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    display: flex;
    flex-direction: column;
    align-items: center;
}

.copybtn{
    width: 440rpx;
      height: 68rpx;
      background-image: linear-gradient(-90deg, 
          #0090ff 0%, 
          #40ccfa 100%), 
      linear-gradient(
          #0090ff, 
          #0090ff);
      background-blend-mode: normal, 
          normal;
    border-radius: 34rpx;
    font-size: 30rpx;
    color: #ffffff;
    text-align: center;
    line-height: 68rpx;
    position: absolute;
    left: 50%;
    bottom: 40rpx;
    transform: translateX(-50%);
  }

.songname{
    text-align: center;
    font-size: 36rpx;
    line-height: 80rpx;
    font-weight: bold;
    width: 90%;
    margin: 0 auto;
}
.songParam{
    background-color: #f8f9fb;
    width: 90%;
    border-radius: 10rpx;
    padding:20rpx;
    word-wrap: break-word;
}
.tip{
    width: 90%;
    font-size: 24rpx;
    color: #999;
    margin-top: 70rpx;
    line-height: 50rpx;
}
.play--kgCD{
    top: 300px;
}